	#header_1>aside>div.bottom>div>ul>li{float:left;width:60px;}
/*楼层floor1 right***/
	#floor_main_right{width:873px;overflow:hidden;margin-left:3px;}
	#floor_main_right>ul{width:876px;overflow:hidden;}
	#floor_main_right>ul>li{float:left;height:238px;width:218px;border-right:1px solid #ddd;border-bottom:1px solid #ddd;}
	#floor_main_right>ul>li>a>img{width:217px;height:234px;} 
	.floor1_bottom{width:1211px;}
	#header_1 .floor1_bottom,.floor1_bottom>ul{overflow:hidden;}
	#header_1 .floor1_bottom>ul>li{float:left;border-right:1px dotted #ddd;padding:5px 9px;}
/******楼层floor2***************/
/*楼层floor2header***/	
	#header_1{overflow:hidden;width:1211px;margin:0 auto;}
	#floor2_main_right{position:relative;}
	#floor2_main_right>ul.banner{width:439px;height:315px;}
	#floor2_main_right>ul.banner>li{float:left;border-left:none;}
	#floor2_main_right .dots { position: absolute; left: 135px;bottom: 10px;}
	#floor2_main_right .dots li { display: inline-block; width: 10px;height: 10px; 
	margin: 0 4px; text-indent: -999em; border: 2px solid #432f21; border-radius: 6px; 
	cursor: pointer; opacity: .4; -webkit-transition: background .5s, opacity .5s; 
	-moz-transition: background .5s, opacity .5s; transition: background .5s, opacity .5s;}
	#floor2_main_right .dots li.active {background: #fff;opacity: 1;}
	#floor2_main_right .arrow { position: absolute; top: 100px;}
	#floor2_main_right #al { left: 15px;}
	#floor2_main_right #ar { right: 15px;}
/*******floor2其他*********/
	.floor2_right_other,.floor2_bottom{float:left;}
	.floor2_right_other>ul>li{float:left;border:1px dotted #ddd;border-left:none;border-top:none;}
	.floor2_right_other>ul,.floor2_bottom>ul{overflow:hidden;}
	.floor2_bottom>ul>li{float:left;border-right:1px dotted #ddd;padding:5px 9px;}
	.floor2_right_other.other{width:872px;margin-left:3px;}
	.floor2_right_other.other img{width:217px;}
/******楼层floor3***************/
	#floor_3_right{position:relative;}
	#floor_3_right>ul.banner{width:439px;height:300px;}
	#floor_3_right>ul.banner>li{float:left;border-left:none;}
	#floor_3_right>ul.banner>li>a>img{height:150px;}
	#floor_3_right .dots { position: absolute; left: 150px;bottom: 10px;}
