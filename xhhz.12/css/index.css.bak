	cursor: pointer; opacity: .4; -webkit-transition: background .5s, opacity .5s; 
	-moz-transition: background .5s, opacity .5s; transition: background .5s, opacity .5s;}
	#floor_3_right .dots li.active {background: #fff;opacity: 1;}
	.floor3_ul1{overflow:hidden;margin-left:41px;}
	.floor3_ul1>li{float:left;border:1px dotted #ddd;border-left:none;border-top:none;height:169px;padding:0 8px;}
	.floor3_ul1>li img{width:150px;height:150px;}
	.floor3_ul2{height:306px;width:429px;margin-left:11px;}
	.floor3_ul2>li{float:left;border:1px dotted #ddd;border-left:none;border-top:none;height:153px;}
	.floor3_ul2>li img{height:153px;}
/*******侧边固定导航栏*****************/
#toolbar{
    position: fixed;
    right: 0;
    top: 0;
    width: 0;
    height: 100%;
    border-right: 6px solid #7a6e6e;
    -webkit-transition: right .3s ease-in-out 0s;
    -moz-transition: right .3s ease-in-out 0s;
    transition: right .3s ease-in-out 0s;
}
#toolbar .toolbar_content{
    position: absolute;
    top: 50%;
    left:-30px;
    width: 35px;
}
.toolbar_tab{
    width: 35px;
    height: 35px;
	left:0px;
    margin-bottom: 1px;
    cursor: pointer;
    background-color: #432f21;
    -webkit-border-radius: 3px 0 0 3px;
    -moz-border-radius: 3px 0 0 3px;
    border-radius: 3px 0 0 3px;	
	z-index:199;
}
.toolbar_tab.hover{
    background-color:#ca171b;
}
.tab-image{
	background-image:url(img/icons/toolbars.png);
	background-repeat:no-repeat;
	display:inline-block;
	width: 34px;
    height: 35px;
    margin-left: 1px;
	position: relative;
    z-index:200;
    background-color:#432f21;
}
.tab-image.hover{
background-color:#ca171b;
}
.tab-icon1{
	background-position:-88px -175px;
}
.tab-icon2{
	background-position:-50px 0px;
}
.tab-icon3{
	background-position:-50px -50px;
}
.tab-icon4{
	background-position:-50px -100px;
}
.tab-icon5{
	background-position:-190px -150px;
}